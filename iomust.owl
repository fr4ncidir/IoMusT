<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/iot/ontologies/2019/5/internet_of_things#"
     xml:base="http://www.semanticweb.org/iot/ontologies/2019/5/internet_of_things"
     xmlns:co="http://purl.org/co#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:iot="http://www.semanticweb.org/iot/ontologies/2019/5/internet_of_things/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:sosa="http://www.w3.org/ns/sosa/"
     xmlns:event="http://purl.org/NET/c4dm/event.owl#"
     xmlns:music="http://purl.org/ontology/mo/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:iomust="http://www.semanticweb.org/iot/ontologies/2019/5/internet_of_things/iomust/"
     xmlns:timeline="http://purl.org/NET/c4dm/timeline.owl#">
    <owl:Ontology rdf:about="http://purl.org/iomust/internet_of_things">
        <owl:versionIRI rdf:resource="http://purl.org/iomust/internet_of_things/0.1"/>
        <dc:contributor>Gyorgy Fazekas</dc:contributor>
        <dc:creator>Francesco Antoniazzi</dc:creator>
        <dc:rights rdf:resource="https://www.gnu.org/licenses/gpl.html"/>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-06-10T10:00:00</terms:created>
        <terms:title>Internet of Musical Things Ontology (IoMusT)</terms:title>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://github.com/fr4ncidir/IoMusT</rdfs:seeAlso>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/rights"/>
    


    <!-- http://purl.org/dc/terms/created -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/co#elementOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/co#elementOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/co#Collection"/>
        <rdfs:comment>This object property connects elements to their collection.</rdfs:comment>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/co#d4e81</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/belongsTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/belongsTo">
        <owl:inverseOf rdf:resource="http://purl.org/iomust/internet_of_things/owns"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/ns/prov#Agent"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Belonging relationship, inverse of ownership</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/connects -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/connects">
        <owl:inverseOf rdf:resource="http://purl.org/iomust/internet_of_things/hasConnection"/>
        <rdfs:domain rdf:resource="http://purl.org/iomust/internet_of_things/Connection"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/ConnectedThing"/>
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/Thing"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Inverse of iot:hasConnection</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/hasConnection -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/hasConnection">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/ConnectedThing"/>
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/Thing"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/iomust/internet_of_things/Connection"/>
        <rdfs:comment>A connected thing points to the connection through this property</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/involves -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/involves">
        <owl:inverseOf rdf:resource="http://purl.org/iomust/internet_of_things/isInvolvedIn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/iomust/internet_of_things/Application"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/Thing"/>
                    <rdf:Description rdf:about="http://www.w3.org/ns/prov#Agent"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>An application involves in the process things and agents (in the broader definition)</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/iomust/internet_of_things/isInvolvedIn"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/isInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/isInvolvedIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/Thing"/>
                    <rdf:Description rdf:about="http://www.w3.org/ns/prov#Agent"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/iomust/internet_of_things/Application"/>
        <rdfs:comment>Inverse of involves.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/iomust/internet_of_things/involves"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/isProducedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/isProducedBy">
        <owl:inverseOf rdf:resource="http://purl.org/iomust/internet_of_things/produces"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
        <rdfs:range rdf:resource="http://purl.org/iomust/internet_of_things/Application"/>
        <rdfs:comment>An application produces some effects on the environment, inverse of iot:produces.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/iomust/internet_of_things/produces"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/owns -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/owns">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/ns/prov#Agent"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:comment>Ownership relationship</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/produces -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/produces">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/iomust/internet_of_things/Application"/>
        <rdfs:range rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
        <rdfs:comment>An application produces some effects on the environment.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/iomust/internet_of_things/isProducedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/runs -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/runs">
        <owl:inverseOf rdf:resource="http://purl.org/iomust/internet_of_things/runsOn"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/SmartThing"/>
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/Thing"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#SoftwareAgent"/>
        <rdfs:comment>Smart things may be able to run software</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/iomust/internet_of_things/runsOn"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/runsOn -->

    <owl:ObjectProperty rdf:about="http://purl.org/iomust/internet_of_things/runsOn">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#SoftwareAgent"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/SmartThing"/>
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/Thing"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Software runs on smart things</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/iomust/internet_of_things/runs"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/iomust/internet_of_things/atLocation -->

    <owl:DatatypeProperty rdf:about="http://purl.org/iomust/internet_of_things/atLocation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>General location indication for things</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/hasBrand -->

    <owl:DatatypeProperty rdf:about="http://purl.org/iomust/internet_of_things/hasBrand">
        <rdfs:domain rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Brand name, like FIAT, Ducati, Boeing, Gibson, Stradivari, and so on...</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/hasModel -->

    <owl:DatatypeProperty rdf:about="http://purl.org/iomust/internet_of_things/hasModel">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Model name of the actual implementation.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/iomust/internet_of_things/hasVersion -->

    <owl:DatatypeProperty rdf:about="http://purl.org/iomust/internet_of_things/hasVersion">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Version numbering of the actual implementation</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/c4dm/event.owl#Event -->

    <owl:Class rdf:about="http://purl.org/NET/c4dm/event.owl#Event">
        <rdfs:comment>Citing from the ontology website, An arbitrary classification of a space/time region, by a cognitive agent. An event may have actively participating agents, passive factors, products, and a location in space/time.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://motools.sourceforge.net/event/event.html"/>
    </owl:Class>
    


    <!-- http://purl.org/co#Collection -->

    <owl:Class rdf:about="http://purl.org/co#Collection">
        <rdfs:comment>Ciccarese, P., &amp; Peroni, S. (2014). The Collections Ontology: creating and handling collections in OWL 2 DL frameworks. Semantic Web, 5(6), 515-529.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/Application -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/Application">
        <rdfs:comment>The semantic endpoint tagging together all elements, items and agents involved in an activity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/ConnectedThing -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/ConnectedThing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/Thing"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/iomust/internet_of_things/hasConnection"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/iomust/internet_of_things/Connection"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:comment>Any thing connected to a communication network.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/iomust/internet_of_things/Connection"/>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/Connection -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/Connection">
        <rdfs:comment>Any connection that allow communication from a thing to another.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/InteractiveApplication -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/InteractiveApplication">
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/Application"/>
        <rdfs:comment>An IoT application that is interactive (i.e., directly communicates with the user)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/LearningApplication -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/LearningApplication">
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/Application"/>
        <rdfs:comment>An application devoted to learning of some kind of concept, technology, art...</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/SmartThing -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/SmartThing">
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:comment>Smart things, e.g., a smartphone, a smart TV, include special technological  features  or artifacts that provide them with relevant added value over the basic version of the same object.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/Thing -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/Thing">
        <rdfs:comment>Any physical object relevant from a user or application perspective.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/WearableThing -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/WearableThing">
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:comment>Any wearable object.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/iomust/MusicalThing -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/iomust/MusicalThing">
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/Thing"/>
        <rdfs:comment>A thing used to produce or enjoy music, with reference to its context.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/iomust/MusicalThingApplication -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/iomust/MusicalThingApplication">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/Application"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/iomust/internet_of_things/involves"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/iomust/internet_of_things/iomust/MusicalThing"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/Application"/>
        <rdfs:comment>If an iot:Application instance is also connected  through iot:isInvolvedIn to an instance of a class belonging to the Music Ontology, or to the iomust namespace, then it is also an instance of iomust:MusicalThingApplication.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/iomust/SmartInstrument -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/iomust/SmartInstrument">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/SmartThing"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/Instrument"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/iomust/MusicalThing"/>
        <rdfs:comment>A smart musical thing that is also an instrument, in the sense given by the Music Ontology</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/ontology/mo/Instrument"/>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/iomust/SmartMusicalThing -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/iomust/SmartMusicalThing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/SmartThing"/>
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/iomust/MusicalThing"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/iomust/internet_of_things/iomust/MusicalThing"/>
        <rdfs:comment>A musical thing that is also a smart thing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/iomust/StageEquipment -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/iomust/StageEquipment">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/co#Collection"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/co#elementOf"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/iomust/internet_of_things/iomust/MusicalThing"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/co#Collection"/>
        <rdfs:comment>A collection of musical things serving as equipment. The definition of collection can be extracted from external ontologies designed ad hoc for this, like the collection ontology by Ciccarese et al.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://purl.org/co#Collection"/>
    </owl:Class>
    


    <!-- http://purl.org/iomust/internet_of_things/iomust/WearableMusicalThing -->

    <owl:Class rdf:about="http://purl.org/iomust/internet_of_things/iomust/WearableMusicalThing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/WearableThing"/>
                    <rdf:Description rdf:about="http://purl.org/iomust/internet_of_things/iomust/MusicalThing"/>
